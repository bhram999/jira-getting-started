{{!< layout}} 
<div class="container">
  <section id="content" role="main">
    <header id="plugin-page-header" class="aui-page-header">
      <div class="aui-page-header-inner">
        <div class="aui-page-header-main">
          <h2>TimeLines</h2>
        </div>
        <div class="aui-page-header-actions">
          <div class="aui-buttons">
            <button class="aui-button aui-button-light">Create Timeline</button>
          </div>
        </div>
      </div>
    </header>
    <div class="aui-page-panel">
      <div id="plugin-content-area" class="aui-page-panel-inner">
        <section class="aui-page-panel-content">
          <!-- main area for content -->
          <table id="plugin-table" class="aui">
            <thead>
              <tr>
                <th id="name">Name</th>
                <th id="filter">Saved Filter</th>
                <th id="visibility">Visibility</th>
                <th id="configure">Configure</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td headers="name">Default</td>
                <td headers="type">Internal Directory</td>
                <td class="order" headers="order">
                  <ul class="menu">
                    <li>
                      <a class="down" href="#">Down</a>
                    </li>
                  </ul>
                </td>
                <td class="action" headers="action">
                  <ul class="menu">
                    <li>
                      <a href="#">An Action</a>
                    </li>
                  </ul>
                </td>
              </tr>
              <tr>
                <td headers="name">Corporate LDAP</td>
                <td headers="type">Microsoft Active Directory</td>
                <td class="order" headers="order">
                  <ul class="menu">
                    <li>
                      <a class="up" href="#">Up</a>
                    </li>
                    <li>
                      <a class="down" href="#">Down</a>
                    </li>
                  </ul>
                </td>
                <td class="action" headers="action">
                  <ul class="menu">
                    <li>
                      <a href="#">An Action</a>
                    </li>
                    <li>
                      <a href="#">Another Action</a>
                    </li>
                  </ul>
                </td>
              </tr>
            </tbody>
          </table>
        </section>
      </div>
    </div>
  </section>
</div>
